{
  "name": "revelo-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev ./src",
    "docker:build": "docker build -f Dockerfile -t revelo-server:dev .",
    "docker:run": "docker run --publish 4000:4000 revelo-server:dev",
    "docker-compose:build": "docker-compose -f docker-compose.yml build",
    "docker-compose:up": "docker-compose -f docker-compose.yml up",
    "start:docker-compose": "npm run docker-compose:build && npm run docker-compose:up",
    "start:docker": "npm run docker:build && npm run docker:run",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": "ryan j. williams",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/merge": "^6.2.7",
    "apollo-datasource-rest": "^0.9.7",
    "apollo-server-express": "^2.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "package-lock-only": "^0.0.4",
    "ramda": "^0.27.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/ramda": "^0.27.36",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
